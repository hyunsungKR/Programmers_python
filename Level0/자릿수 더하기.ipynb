   "source": [
    "# 문제 설명\n",
    "# 정수 n이 매개변수로 주어질 때 n의 각 자리 숫자의 합을 return하도록 solution 함수를 완성해주세요"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n) :\n",
    "    answer = 0\n",
    "    \n",
    "    for i in range(len(str(n))) :\n",
    "        answer += int(str(n)[i])\n",
    "        \n",
    "    return answer"
   ]
  },
